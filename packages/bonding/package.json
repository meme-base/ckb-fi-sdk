{
  "name": "@ckb-fi/bonding",
  "version": "0.0.1-beta.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meme-base/ckb-fi-sdk.git"
  },
  "homepage": "https://ckb.fi",
  "keywords": [
    "ckb",
    "ckb-fi",
    "bonding",
    "sdk"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "publish": "npm run build && npm publish",
    "lint": "eslint \"**/*.ts\""
  },
  "exports": {
    ".": "./src/ckb-fi-bonding.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/ckb-fi-bonding.cjs",
    "module": "./dist/ckb-fi-bonding.mjs",
    "types": "./dist/ckb-fi-bonding.d.ts",
    "exports": {
      ".": {
        "types": "./dist/ckb-fi-bonding.d.ts",
        "import": "./dist/ckb-fi-bonding.mjs",
        "require": "./dist/ckb-fi-bonding.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@ckb-fi/eslint-config": "workspace:*",
    "@ckb-fi/typescript-config": "workspace:*",
    "@ckb-fi/utils": "workspace:*",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "events": "^3.3.0"
  }
}
